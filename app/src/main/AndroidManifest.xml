<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajan.apps.modulicalculator"
    android:versionCode="1"
    android:versionName="1.0">


    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="25"
        />




    <uses-permission android:name="android.permission.RECORD_AUDIO"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_3"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"

        >



        <activity
            android:name="com.rajan.apps.modulicalculator.Welcome_Screen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.rajan.apps.modulicalculator.Calculator"
            android:parentActivityName= "com.rajan.apps.modulicalculator.Welcome_Screen">
        </activity>


        <activity
            android:name="com.rajan.apps.modulicalculator.Help_Doc"
            android:parentActivityName= "com.rajan.apps.modulicalculator.Welcome_Screen">
        </activity>
    </application>


</manifest>
